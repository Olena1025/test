// Place your key bindings in this file to override the defaultsauto[]
[
  {
    "key": "ctrl+shift+enter",
    "command": "macros.getOutTheMethod",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+/",
    "command": "macros.commentLine",
    "when": "editorTextFocus"
  },
  {
    "key": "alt+f",
    "command": "toggleSearchEditorContextLines",
    "when": "inSearchEditor"
  },
  {
    "key": "alt+l",
    "command": "-toggleSearchEditorContextLines",
    "when": "inSearchEditor"
  },
  {
    "key": "alt+f",
    "command": "toggleFindInSelection",
    "when": "editorFocus"
  },
  {
    "key": "alt+l",
    "command": "-toggleFindInSelection",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+l",
    "command": "-expandLineSelection",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "-editor.action.insertLineAfter",
    "when": "editorTextFocus && !editorReadonly && !suggestWidgetVisible"
  },
  {
    "key": "ctrl+/",
    "command": "-editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFile"
  },
  {
    "key": "ctrl+alt+n",
    "command": "explorer.newFolder"
  },
  {
    "key": "alt+`",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "alt+f12",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminalFocus"
  },
  {
    "key": "alt+`",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+alt+c",
    "command": "local-history.compareToCurrent"
  },
  {
    "key": "ctrl+alt+h",
    "command": "treeLocalHistoryExplorer.focus"
  },
  {
    "key": "alt+9",
    "command": "-workbench.view.scm",
    "when": "editorFocus"
  },
  {
    "key": "alt+9",
    "command": "-workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+shift+z",
    "command": "redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+y",
    "command": "-redo",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+l",
    "command": "-notebook.centerActiveCell",
    "when": "notebookEditorFocused && activeEditor == 'workbench.editor.notebook'"
  },
  {
    "key": "ctrl+l",
    "command": "-notebook.centerActiveCell",
    "when": "notebookEditorFocused"
  },
  {
    "key": "ctrl+l",
    "command": "editor.action.fixAll",
    "when": "editorTextFucus"
  },
  {
    "key": "alt+2",
    "command": "-workbench.action.openEditorAtIndex2"
  },
  {
    "key": "alt+3",
    "command": "workbench.scm.focus"
  },
  {
    "key": "alt+q",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "editorFocus || !editorFocus"
  },
  {
    "key": "alt+1",
    "command": "-workbench.action.toggleSidebarVisibility",
    "when": "!editorFocus"
  },
  {
    "key": "ctrl+alt+d",
    "command": "openInTerminal"
  },
  {
    "key": "ctrl+alt+r",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "ctrl+alt+l",
    "command": "-editor.action.formatSelection",
    "when": "editorHasDocumentFormattingProvider && editorHasSelection && editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+alt+e",
    "command": "eslint.executeAutofix"
  },
  {
    "key": "ctrl+shift+d",
    "command": "-workbench.view.debug"
  },
  {
    "key": "ctrl+shift+space",
    "command": "acceptSelectedSuggestion",
    "when": "editorFocus && suggestWidgetVisible"
  },
  {
    "key": "ctrl+shift+enter",
    "command": "-acceptSelectedSuggestion",
    "when": "editorFocus && suggestWidgetVisible"
  },
  {
    "key": "shift+space",
    "command": "acceptAlternativeSelectedSuggestion",
    "when": "suggestWidgetVisible && textInputFocus && textInputFocus"
  },
  {
    "key": "shift+enter",
    "command": "-acceptAlternativeSelectedSuggestion",
    "when": "suggestWidgetVisible && textInputFocus && textInputFocus"
  },
  {
    "key": "alt+3",
    "command": "-workbench.action.toggleSidebarVisibility",
    "when": "searchViewletVisible"
  },
  {
    "key": "alt+2",
    "command": "workbench.view.search",
    "when": "!searchViewletVisible"
  },
  {
    "key": "alt+3",
    "command": "-workbench.view.search",
    "when": "!searchViewletVisible"
  },
  {
    "key": "alt+3",
    "command": "-workbench.action.openEditorAtIndex3"
  },
  {
    "key": "ctrl+p",
    "command": "-editor.action.triggerParameterHints",
    "when": "editorHasSignatureHelpProvider && editorTextFocus"
  },
  {
    "key": "ctrl+k",
    "command": "editor.foldAllBlockComments",
    "when": "editorTextFocus && foldingEnabled"
  },
  {
    "key": "ctrl+k ctrl+/",
    "command": "-editor.foldAllBlockComments",
    "when": "editorTextFocus && foldingEnabled"
  }
]
